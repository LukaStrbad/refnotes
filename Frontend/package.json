    "@angular/platform-browser": "^19.2.4",
    "@angular/platform-browser-dynamic": "^19.2.4",
    "@angular/router": "^19.2.4",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "bootstrap-icons": "^1.11.3",
    "highlight.js": "^11.11.1",
    "jwt-decode": "^4.0.0",
    "lru-cache": "^11.1.0",
    "marked": "^15.0.7",
