{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "pnpm dlx @tailwindcss/cli -i ./src/styles.css -o ./dist/test.css && ng test",
    "test:coverage": "ng test --no-watch --code-coverage",
    "test:ci": "pnpm dlx @tailwindcss/cli -i ./src/styles.css -o ./dist/test.css && ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint"
  },
  "private": true,
